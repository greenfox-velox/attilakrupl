def anagramm(string1, string2):
 
    if len(string1) >= 2 and j(string1, string2) or string1 is string2: 
        return True
    return False

def j(string1, string2):
    j = True
    for i in string1:
        if i in string2:
            j = j and True
        else:
            j = j and False
    return j