from math import acos
from math import degrees

def sideOrder(atf, bef1, bef2): 
    if atf < bef1:
            x = atf
            atf = bef1
            bef1 = x
    else:
        pass
    if atf < bef2:
        x = atf
        atf = bef2
        bef2 = x
    else:
        pass
    if bef1 < bef2:
        x = bef1
        bef1 = bef2
        bef2 = x
    else:
        pass
    return (atf, bef1, bef2)

def triangle_type(a, b, c):
    x = 0
    
     
    if b + c <= a:
        return 0
    else:
        gamma = degrees((acos(((bef1**2)+(bef2**2)-(atf**2))/(2*bef1*bef2))))
        if gamma < 90:
            return 1
        if gamma == 90:
            return 2
        else:
            return 3
         
     
     
print (triangle_type(3,4,6))