def iq_test(numbers):
    new = numbers.split()
    even = {}
    evenc = 0
    odd = {}
    for i in range(len(new)):
        if new[i] % 2 == 0:
            even += {i:"new[i]"}
        else:
            odd += {i:"new[i]"}
    
    if len(even) < len(odd):
        return even
    
